<?xml version="1.0"?>
<registry>
  <record name="collective.documentfusion.interfaces.ISOfficeSettings.port"
          interface="collective.documentfusion.interfaces.ISOfficeSettings"
          field="port">
    <field type="plone.registry.field.Int">
      <default>2002</default>
      <description>LibreOffice port</description>
      <required>True</required>
      <title>The port used by LibreOffice service</title>
    </field>
    <value>2002</value>
  </record>
  <record name="collective.documentfusion.interfaces.ISOfficeSettings.host"
          interface="collective.documentfusion.interfaces.ISOfficeSettings"
          field="host">
    <field type="plone.registry.field.ASCIILine">
      <default>localhost</default>
      <description>LibreOffice host</description>
      <required>True</required>
      <title>The host name of LibreOffice service</title>
    </field>
    <value>localhost</value>
  </record>
  <record name="collective.documentcompare.interfaces.ISOfficeSettings.uno_python"
          interface="collective.documentcompare.interfaces.ISOfficeSettings"
          field="host">
    <field type="plone.registry.field.ASCIILine">
      <default>localhost</default>
      <description>Uno python to use</description>
      <required>True</required>
      <title>The python command where uno can be imported (can be python 2 or 3)</title>
    </field>
    <value>localhost</value>
  </record>
</registry>
